+    const [messageInput, setMessageInput] = useState('');
+    const [onlineCount, setOnlineCount] = useState(0);
+    const [cursors, setCursors] = useState<Map<string, CursorData>>(new Map());
+    const [loading, setLoading] = useState(true);
