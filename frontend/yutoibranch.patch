+    font-size: 24px;
+    font-weight: bold;
+    background: linear-gradient(90deg, #521575, #AE74CE, #EC7A97, #B1224B);
+    -webkit-background-clip: text;
+    background-clip: text;
+    -webkit-text-fill-color: transparent;
+    margin: 0;
+    flex-shrink: 0;
+}
+
+.search-form-header {
+    flex: 1;
+    max-width: 600px;
+}
+
+.search-box-header {
+    display: flex;
+    align-items: center;
+    border: 1px solid #dfe1e5;
+    border-radius: 24px;
+    padding: 10px 15px;
+    background-color: #fff;
+}
+
+.search-box-header:focus-within {
+    box-shadow: 0 1px 6px rgba(32, 33, 36, 0.28);
+    border-color: rgba(223, 225, 229, 0);
+}
+
+.search-icon {
+    color: #9aa0a6;
+    margin-right: 10px;
+}
+
+.search-input-header {
+    flex: 1;
+    border: none;
+    outline: none;
+    font-size: 16px;
+}
+
+.search-clear {
+    background: none;
+    border: none;
+    font-size: 24px;
+    color: #70757a;
+    cursor: pointer;
+    padding: 0;
+    margin-left: 10px;
+}
+
+.search-clear:hover {
+    color: #202124;
+}
+
+.search-page-main {
+    max-width: 800px;
+    margin: 0 auto;
+    padding: 20px;
+}
+
+.loading {
+    text-align: center;
+    padding: 40px;
+    color: #70757a;
+    font-size: 16px;
+}
+
+.error-message {
+    padding: 15px;
+    background-color: #fee;
+    color: #c33;
+    border-radius: 8px;
+    margin-bottom: 20px;
+}
+
+.results-info {
+    color: #70757a;
+    font-size: 14px;
+    margin-bottom: 20px;
+    padding-left: 5px;
+}
+
+.no-results {
+    padding: 40px 20px;
+    text-align: center;
+    color: #70757a;
+}
+
+.search-results {
+    margin-top: 20px;
+}
+
+.search-result-item {
+    margin-bottom: 36px;
+    padding-left: 5px;
+}
+
+.result-url {
+    color: #202124;
+    font-size: 14px;
+    line-height: 1.3;
+    margin-bottom: 3px;
+}
+
+.result-title {
+    margin: 0 0 3px 0;
+    line-height: 1.3;
+}
+
+.result-title a {
+    color: #1a0dab;
+    text-decoration: none;
+    font-size: 20px;
+    font-weight: normal;
+    cursor: pointer;
+}
+
+.result-title a:hover {
+    text-decoration: underline;
+}
+
+.result-title a:visited {
+    color: #681da8;
+}
+
+.result-snippet {
+    color: #4d5156;
+    font-size: 14px;
+    line-height: 1.58;
+    margin: 0;
+    word-wrap: break-word;
+}
+
+/* „É¨„Çπ„Éù„É≥„Ç∑„Éñ */
+@media (max-width: 768px) {
+    .header-content {
+        flex-direction: column;
+        gap: 15px;
+    }
+
+    .search-form-header {
+        width: 100%;
+        max-width: 100%;
+    }
+
+    .search-page-main {
+        padding: 15px;
+    }
+}
