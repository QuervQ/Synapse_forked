+                    <div
+                        key={userId}
+                        className="cursor"
+                        style={{
+                            left: `${data.x}px`,
+                            top: `${data.y}px`,
+                            backgroundColor: data.color,
+                        }}
+                        data-user={data.user}
+                    />
+                ))}
+            </div>
+
+            <div className="chat-container">
+                <div className="chat-header">
+                    <span>üí¨ {roomId}</span>
